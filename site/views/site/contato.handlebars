   <section class="contact">
        <div class="max-width">
            <h2 class="title">{{contentContact.titleContact}}</h2>
            <div class="contact-content">
                <div class="column left">
                    <p>{{contentContact.descContact}}</p>
                    <div class="icons">
                        <div class="row">
                            <i class="{{contentContact.iconCompany}}"></i>
                            <div class="info">
                                <div class="head">
                                    {{contentContact.titleCompany}}
                                </div>
                                <div class="sub-title">
                                    {{contentContact.descCompany}}
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <i class="{{contentContact.iconAddress}}"></i>
                            <div class="info">
                                <div class="head">
                                    {{contentContact.titleAddress}}
                                </div>
                                <div class="sub-title">
                                    {{contentContact.descAddress}}
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <i class="fa-solid fa-envelope"></i>
                            <div class="info">
                                <div class="head">
                                    {{contentContact.titleEmail}}
                                </div>
                                <div class="sub-title">
                                    {{contentContact.descEmail}}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="column right">
                    <div class="text">
                        {{contentContact.titleForm}}
                    </div>
                   {{>_msg}}

                    <form action="/contato" method="post">
                        <div class="fields">
                            <div class="field name">
                                <input type="text" name="name" id="name" placeholder="Digite o nome" value="{{data.name}}" required>
                            </div>
                            <div class="field email">
                                <input type="email" name="email" id="email" placeholder="Digite o e-mail" value="{{data.email}}" required>
                            </div>
                        </div>

                        <div class="field">
                            <input type="text" name="subject" id="subject" placeholder="Digite o assunto" value="{{data.subject}}" required>
                        </div>

                        <div class="field textarea">
                            <textarea name="content" id="content" cols="30" rows="10" placeholder="Digite o conteÃºdo" required>{{data.content}}</textarea>
                        </div>

                        <div class="button-area">
                            <button type="submit">Enviar</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>