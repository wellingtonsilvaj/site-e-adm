<!-- Inicio do conteudo do administrativo -->
<div class="wrapper">
    <div class="row">
        <div class="top-list">
            <span class="title-content">VISUALIZAR</span>
            <div class="top-list-right">
                <a href="#" class="btn-warning">Editar</a>
                
                
            </div>
        </div>

        {{>_msgAdm}}

        <div class="content-adm">

            

            <div class="view-det-adm">
                <span class="view-adm-title">Nome: </span>
                <span class="view-adm-info">{{contactsMsgs.dataValues.name}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">E-mail: </span>
                <span class="view-adm-info">{{contactsMsgs.dataValues.email}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Assunto: </span>
                <span class="view-adm-info">{{contactsMsgs.dataValues.subject}}</span>
            </div>
            
            <div class="view-det-adm">
                <span class="view-adm-title">Conte√∫do: </span>
                <span class="view-adm-info">{{contactsMsgs.dataValues.content}}</span>
            </div>

            <div class="view-det-adm">
                <span class="view-adm-title">Cadastrado: </span>
                <span class="view-adm-info">
                    {{#formatDateTime user.dataValues.createdAt}}{{/formatDateTime}}
                </span>
            </div>

            <div class="view-det-adm">
                <span class="view-adm-title">Editado: </span>
                <span class="view-adm-info">
                    {{#formatDateTime user.dataValues.updatedAt}}{{/formatDateTime}}
                </span>
            </div>
            
        </div>
    </div>
</div>
<!-- Fim do conteudo do administrativo -->