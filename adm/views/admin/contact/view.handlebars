<!-- Inicio do conteudo do administrativo -->
<div class="wrapper">
    <div class="row">
        <div class="top-list">
            <span class="title-content">PÁGINA DE CONTATOS</span>
            <div class="top-list-right">
                <a href="/contact-site/edit" class="btn-warning">Editar</a>
                
                
            </div>
        </div>

        {{>_msgAdm}}

        <div class="content-adm">

            

            <div class="view-det-adm">
                <span class="view-adm-title">Título: </span>
                <span class="view-adm-info">{{contentContact.dataValues.titleContact}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Descrição: </span>
                <span class="view-adm-info">{{contentContact.dataValues.descContact}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Ícone Sobre Empresa: </span>
                <span class="view-adm-info">{{contentContact.dataValues.iconCompany}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Título Sobre Empresa: </span>
                <span class="view-adm-info">{{contentContact.dataValues.titleCompany}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Descrição Sobre Empresa: </span>
                <span class="view-adm-info">{{contentContact.dataValues.descCompany}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Ícone do Endereço: </span>
                <span class="view-adm-info">{{contentContact.dataValues.iconAddress}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Título do Endereço: </span>
                <span class="view-adm-info">{{contentContact.dataValues.titleAddress}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Descrição do Endereço: </span>
                <span class="view-adm-info">{{contentContact.dataValues.descAddress}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Ícone do Email: </span>
                <span class="view-adm-info">{{contentContact.dataValues.iconEmail}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Título do Email: </span>
                <span class="view-adm-info">{{contentContact.dataValues.titleEmail}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Descrição do E-mail: </span>
                <span class="view-adm-info">{{contentContact.dataValues.descEmail}}</span>
            </div>

            
            <div class="view-det-adm">
                <span class="view-adm-title">Título da Mensagem: </span>
                <span class="view-adm-info">{{contentContact.dataValues.titleForm}}</span>
            </div>

            <div class="view-det-adm">
                <span class="view-adm-title">Cadastrado: </span>
                <span class="view-adm-info">
                    {{#formatDateTime user.dataValues.createdAt}}{{/formatDateTime}}
                </span>
            </div>

            <div class="view-det-adm">
                <span class="view-adm-title">Editado: </span>
                <span class="view-adm-info">
                    {{#formatDateTime user.dataValues.updatedAt}}{{/formatDateTime}}
                </span>
            </div>
            
        </div>
    </div>
</div>
<!-- Fim do conteudo do administrativo -->