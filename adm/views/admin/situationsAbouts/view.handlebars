<!-- Inicio do conteudo do administrativo -->
<div class="wrapper">
    <div class="row">
        <div class="top-list">
            <span class="title-content">VISUALIZAR</span>
            <div class="top-list-right">
                <a href="/situations-abouts-site/edit/{{situationsAbouts.dataValues.id}}" class="btn-warning">Editar</a>
                <a href="/situations-abouts-site?page=1" class="btn-info">Listar</a>
                <a href="/situations-abouts-site/delete/{{situationsAbouts.dataValues.id}}" class="btn-danger" onclick="return confirm('Tem certeza que deseja excluir este registro?')">Apagar</a> 
                
            </div>
        </div>

        {{>_msgAdm}}

        <div class="content-adm">

            

            <div class="view-det-adm">
                <span class="view-adm-title">Situação: </span>
                <span class="view-adm-info">{{situationsAbouts.dataValues.nameSituation}}</span>
            </div>

       

            <div class="view-det-adm">
                <span class="view-adm-title">Cadastrado: </span>
                <span class="view-adm-info">
                    {{#formatDateTime user.dataValues.createdAt}}{{/formatDateTime}}
                </span>
            </div>

            <div class="view-det-adm">
                <span class="view-adm-title">Editado: </span>
                <span class="view-adm-info">
                    {{#formatDateTime user.dataValues.updatedAt}}{{/formatDateTime}}
                </span>
            </div>
            
        </div>
    </div>
</div>
<!-- Fim do conteudo do administrativo -->